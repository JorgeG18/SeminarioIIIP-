ad * prod.precio) + acum, 0)\r\n    let impto = subTotal * 0.15\r\n    let totalPagar = subTotal + impto\r\n\r\n    useEffect(() => {\r\n\r\n    }, [renderizar])\r\n\r\n    const handleBorrarProduct = (x) =>{\r\n\r\n        productos.map((prod, index, obj) => {\r\n            \r\n            if(prod.id === x.id){\r\n                obj.splice(index, 1)\r\n                alert('Confirmar') //Recordatorio: Agregar la confirmaciÃ³n\r\n\r\n            }\r\n            return prod\r\n        })\r\n        setRenderizar(!renderizar)\r\n    }\r\n\r\n    const handleMostrarCarro = _ => setMostrarCarro(!mostrarCarro)\r\n\r\n    return (\r\n        <CarroContenedor>\r\n            {cantidad > 0 && <Burbuja cantidad={cantidad} />}\r\n            <Carrito onClick={handleMostrarCarro}>\r\n                Carro\r\n            </Carrito>\r\n            {\r\n                (cantidad > 0 && mostrarCarro) && \r\n                <ListaArticulos>\r\n                        <Ul>\r\n                            {\r\n                                productos.map(x => {\r\n                                    return (\r\n                                        <Li>\r\n                                            <img height={25} alt={x.nombre} src={x.imagen} />\r\n                                            \r\n                                            <span><DeleteButton  onClick={() => handleBorrarProduct(x)} >X</DeleteButton> {x.nombre}</span>\r\n                                            \r\n                                            <span>\r\n                                                ({x.cantidad} x {x.precio.toLocaleString()}) = <strong>{(x.cantidad * x.precio).toLocaleString()}</strong>\r\n                                            </span>\r\n                                        </Li>\r\n                                    )\r\n                                })\r\n                            }\r\n                            <Li>\r\n                                <strong>Sub total</strong>\r\n                                <strong>{subTotal.toLocaleString()}</strong>\r\n                            </Li>\r\n                            <Li>\r\n                                <strong>Impuesto</strong>\r\n                                <strong>{impto.toLocaleString()}</strong>\r\n                            </Li>\r\n                            <Li>\r\n                                <strong>Total a pagar</strong>\r\n                                <strong>{totalPagar.toLocaleString()}</strong>\r\n                            </Li>\r\n                        </Ul>\r\n                </ListaArticulos>\r\n                    \r\n            }\r\n        </CarroContenedor>\r\n\r\n    )\r\n}","C:\\Users\\Jorge'G\\OneDrive\\Documentos\\Tercer Periodo 2021\\Seminario-Taller de Software\\IIIP\\react-sample-1\\src\\components\\Articulo\\styles.js",[],"C:\\Users\\Jorge'G\\OneDrive\\Documentos\\Tercer Periodo 2021\\Seminario-Taller de Software\\IIIP\\react-sample-1\\src\\components\\Burbuja\\index.js",[],"C:\\Users\\Jorge'G\\OneDrive\\Documentos\\Tercer Periodo 2021\\Seminario-Taller de Software\\IIIP\\react-sample-1\\src\\components\\Articulos\\styles.js",[],"C:\\Users\\Jorge'G\\OneDrive\\Documentos\\Tercer Periodo 2021\\Seminario-Taller de Software\\IIIP\\react-sample-1\\src\\components\\Burbuja\\styles.js",[],"C:\\Users\\Jorge'G\\OneDrive\\Documentos\\Tercer Periodo 2021\\Seminario-Taller de Software\\IIIP\\react-sample-1\\src\\components\\Button\\styles.js",[],"C:\\Users\\Jorge'G\\OneDrive\\Documentos\\Tercer Periodo 2021\\Seminario-Taller de Software\\IIIP\\react-sample-1\\src\\components\\Carro\\styles.js",[],"C:\\Users\\Jorge'G\\OneDrive\\Documentos\\Tercer Periodo 2021\\Seminario-Taller de Software\\IIIP\\react-sample-1\\src\\components\\Navbar\\styles.js",[],{"ruleId":"72","replacedBy":"73"},{"ruleId":"74","replacedBy":"75"},{"ruleId":"76","severity":1,"message":"77","line":12,"column":12,"nodeType":"78","messageId":"79","endLine":12,